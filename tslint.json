{
  "rulesDirectory": [
    "tslint-plugin-prettier",
    "dtslint/bin/rules/expectRule.js"
  ],
  "extends": [
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "rules": {
    "expect": true,
    "member-access": true,
    "no-boolean-literal-compare": false,
    "no-console": false,
    "no-debugger": true,
    "no-empty-interface": true,
    "no-internal-module": true,
    "no-unused-variable": true,
    "object-shorthand-properties-first": false,
    "prefer-for-of": true,
    "prettier": [
      true,
      {
        "singleQuote": true,
        "trailingComma": "all",
        "bracketSpacing": true,
        "printWidth": 90
      }
    ],
    "import-name": false
  }
}
